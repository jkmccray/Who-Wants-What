@model WhoWantsWhat.Models.ViewModels.ItemsViewModels.AddExistingItemToWishListViewModel

@{
    ViewData["Title"] = "AddExistingItemToWishList";
}

<h1>Select List</h1>


<form asp-action="SaveExistingItemToWishList">
    <input type="hidden" asp-for="ItemId" />
    <div class="form-group">
        <label asp-for="WishListId" class="control-label"></label>
        <select asp-for="WishListId" class="form-control" asp-items="@Model.WishListOptions"></select>
        <span asp-validation-for="WishListId" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Save" class="btn btn-primary" />
    </div>
</form>
